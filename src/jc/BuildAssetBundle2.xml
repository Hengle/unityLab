<?xml version="1.0" encoding="utf-8"?>
<buildAssetBundle ignoreExtention="svn;meta" platform="StandaloneWindows">
	
	<saveRoot><![CDATA[u3dRes]]></saveRoot>
	
	<asset id="ngui_atlas_font">
		<bind savePath="ui/ngui.unity3d">
			<item type="directory" recursion="true"><![CDATA[NGUI]]></item>
		</bind>
		<bind savePath="ui/ui_atlas.unity3d">
			<item type="file"><![CDATA[Atlas/UIAtlas.prefab]]></item>
			<item type="file"><![CDATA[Atlas/UIAtlas.mat]]></item>
			<item type="file"><![CDATA[Atlas/UIAtlas.png]]></item>
		</bind>
		<bind savePath="ui/ui_font.unity3d">
			<item type="file"><![CDATA[Font/msyh.prefab]]></item>
			<item type="file"><![CDATA[Font/msyh.ttf]]></item>
		</bind>
	</asset>

	<asset id="loading" dependence="ngui_atlas_font">
		<bind savePath="ui/loading.unity3d">
			<item type="file"><![CDATA[Loading/Prefab/Loading.prefab]]></item>
		</bind>
	</asset>
	
</buildAssetBundle>